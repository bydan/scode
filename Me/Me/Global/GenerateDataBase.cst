public string GetXmlNavigationRuleFace(TableSchema TablaBase) {
	
	String strXmlNavigationRulesFaces="";
	
	String strTablaClaseRelacionada=string.Empty;
			System.Collections.Hashtable tablasRelacionadas=GetTablasRelacionadas(TablaBase);
			
			TableSchema tablaRelacionadaObjetivo;
			String strPlural=string.Empty;
			ArrayList tablasRelacionadasEncontradas=new ArrayList();
			bool encontrado=false;
			bool existe=false;
			
			String strSufijoJSPFinalTable="";
			String strSufijoJSPFinalTableRelacionada="";
			
			if(GetFinalJspVersionFromPropertiesC(TablaBase)) {
				strSufijoJSPFinalTable=strSufijoJSPJSFFinal;
			}
			
			strTablaClaseRelacionada+="\r\n\r\n<navigation-rule>";
			strTablaClaseRelacionada+="\r\n<from-view-id>/Paginas/"+GetRelativePathNavegacionFromModuleActualC(TablaBase)+GetRelativePathNavegacionFromTableFromPropertiesC(TablaBase)+""+GetNombreClaseC(TablaBase.ToString())+strViewMantenimiento+strSufijoJSPFinalTable+"."+strExtensionWeb+"</from-view-id>";
							
			foreach(CollectionInfo collectionInfo in tablasRelacionadas.Values)	{
				
				if(collectionInfo.CollectionRelationshipType==RelationshipType.OneToOne){
					tablaRelacionadaObjetivo=collectionInfo.SecondaryTableSchema;
					
				} else if(collectionInfo.CollectionRelationshipType==RelationshipType.OneToMany) {					
					tablaRelacionadaObjetivo=collectionInfo.SecondaryTableSchema;
				
				} else {
					tablaRelacionadaObjetivo=collectionInfo.JunctionTableSchema;
				}
				
				encontrado=false;
				
				foreach(TableSchema tableSchema in tablasRelacionadasEncontradas) {
					if(tableSchema.Equals(tablaRelacionadaObjetivo)) {
						encontrado=true;
					}
				}
							
				if(!encontrado)	{
					tablasRelacionadasEncontradas.Add(tablaRelacionadaObjetivo);
					
					if(!existe) {
						existe=true;
					}
					//strPlural=GetPluralTituloNombreTableFromPropertiesC(tablaRelacionadaObjetivo);
					
					strSufijoJSPFinalTableRelacionada="";
					
					if(GetFinalJspVersionFromPropertiesC(tablaRelacionadaObjetivo)) {
						strSufijoJSPFinalTableRelacionada=strSufijoJSPJSFFinal;
					}
			
					strTablaClaseRelacionada+="\r\n\t<navigation-case>";
					strTablaClaseRelacionada+="\r\n\t\t<from-outcome>"+GetNombreClaseObjetoC(tablaRelacionadaObjetivo.ToString())+"s</from-outcome>";
					strTablaClaseRelacionada+="\r\n\t\t<to-view-id>/Paginas/"+GetRelativePathNavegacionFromModuleActualC(tablaRelacionadaObjetivo)+GetRelativePathNavegacionFromTableFromPropertiesC(tablaRelacionadaObjetivo)+""+GetNombreClaseC(tablaRelacionadaObjetivo.ToString())+strViewMantenimiento+strSufijoJSPFinalTableRelacionada+"."+strExtensionWeb+"</to-view-id>";
					strTablaClaseRelacionada+="\r\n\t\t<redirect/>";
					strTablaClaseRelacionada+="\r\n\t</navigation-case>";
					
					
				}
				
			}
			//Trace.WriteLine("Nav:"+TablaBase.Name);
			
			//NAVEGACION HACIA LOS FK
			strTablaClaseRelacionada+=GetXmlNavigationRuleFaceFK(TablaBase,ref existe);
			
			strTablaClaseRelacionada+="\r\n</navigation-rule>";
			
			if(existe) {
				strXmlNavigationRulesFaces=strTablaClaseRelacionada;
			}
			
	return strXmlNavigationRulesFaces;
}

public string GetXmlNavigationRuleFaceFK(TableSchema TablaBase,ref bool existe) {
	String strTablaClaseRelacionada=string.Empty;
	TableSchema tableSchema=null;
	String strSufijoJSPFinalTableFK="";
	
	foreach(ColumnSchema columnSchema in TablaBase.Columns) {	
		if(columnSchema.IsForeignKeyMember&&!columnSchema.Name.Equals(strId+TablaBase.Name)) {
			existe=true;
			//Trace.WriteLine("Nav-Rel:"+columnSchema.Name);
			tableSchema=GetNombreTablaRelacionadaFromColumn(columnSchema);
			
			strSufijoJSPFinalTableFK="";
					
			if(GetFinalJspVersionFromPropertiesC(tableSchema)) {
				strSufijoJSPFinalTableFK=strSufijoJSPJSFFinal;
			}
					
			strTablaClaseRelacionada+="\r\n\t<navigation-case>";
			strTablaClaseRelacionada+="\r\n\t\t<from-outcome>"+GetNombreClaseObjetoC(tableSchema.ToString())+"</from-outcome>";
			strTablaClaseRelacionada+="\r\n\t\t<to-view-id>/Paginas/"+GetRelativePathNavegacionFromModuleActualC(tableSchema)+GetRelativePathNavegacionFromTableFromPropertiesC(tableSchema)+""+GetNombreClaseC(tableSchema.ToString())+strViewMantenimiento+strSufijoJSPFinalTableFK+"."+strExtensionWeb+"</to-view-id>";
			strTablaClaseRelacionada+="\r\n\t\t<redirect/>";
			strTablaClaseRelacionada+="\r\n\t</navigation-case>";
		}
	}
	
	return  strTablaClaseRelacionada;
}

