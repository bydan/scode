public String GetIGridyPrincipal(bool conAcumulador) {
	String sReturn="iGridyPrincipal";//iGridyRelaciones++
	
	if(!blnConForzarUbicarPanelesManualmente) {
		if(conAcumulador) {
			sReturn+="++";
		}
	} else {
		iGridyPrincipal++;
		sReturn=iGridyPrincipal.ToString();
	}
	
	//iGridyRelaciones
	
	return sReturn;
}

