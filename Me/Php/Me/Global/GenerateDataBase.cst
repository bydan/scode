				{
					
					strTablaClaseRelacionada+="\r\n\r\n<servlet>";
					strTablaClaseRelacionada+="\r\n<display-name>"+GetNombreClaseC(table.ToString())+GetNombreColumnaClaseC(columnSchema)+"ImagenServlet</display-name>";
