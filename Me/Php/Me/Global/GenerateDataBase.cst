					strTablaClaseRelacionada+="\r\n\r\n<servlet-mapping>";
					strTablaClaseRelacionada+="\r\n<servlet-name>"+GetNombreClaseC(table.ToString())+GetNombreColumnaClaseC(columnSchema)+"ImagenServlet</servlet-name>";
