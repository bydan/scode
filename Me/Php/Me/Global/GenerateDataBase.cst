public String GetStringStoreProcedures() 
{
	String strStoreProcedures="";
	
	foreach(TableSchema tableSchema in SourceDatabaseTables)
	{
		strStoreProcedures+="DELIMITER $$\r\n\r\n";
		
		strStoreProcedures+="DROP PROCEDURE IF EXISTS `"+GetSchemaTableFromProperties(tableSchema,ConexionSchema)+"`.`SP_"+GetNombreClaseC(tableSchema.ToString()).ToUpper()+"_INSERT` $$\r\n\r\n";
		
		strStoreProcedures+="CREATE PROCEDURE `"+GetSchemaTableFromProperties(tableSchema,ConexionSchema)+"`.`SP_"+GetNombreClaseC(tableSchema.ToString()).ToUpper()+"_INSERT`("; 
		
		for (int i = 0; i < tableSchema.Columns.Count; i++){ strStoreProcedures+=GetSqlStoreProcedureParamParameterInsert(tableSchema.Columns[i]) ; if (i < tableSchema.Columns.Count - 1&&i!=0&&tableSchema.Columns[i].Name!=strVersionRow) { strStoreProcedures+=","; }  } strStoreProcedures+=", OUT intEstadoProceso int(10))\r\n";
		
